{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Root Schema",
    "required": [
        "title",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 40
        },
        "salary": {
            "type": "integer",
            "minimum": 1
        },
        "equity": {
            "type": "number",
            "maximum": 1.0
        },
        "companyHandle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 25
        }
    },
    "additionalProperties": false
}